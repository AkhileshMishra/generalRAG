<?xml version="1.0" encoding="utf-8" ?>
<services version="1.0">
    <container id="default" version="1.0">
        <document-api/>
        <search>
            <chain id="default" inherits="vespa">
                <searcher id="ai.vespa.search.llm.RAGSearcher" bundle="llm-rag"/>
            </chain>
        </search>
        <document-processing/>
        <nodes>
            <node hostalias="node1"/>
        </nodes>
    </container>

    <content id="sop_content" version="1.0">
        <redundancy>1</redundancy>
        <documents>
            <document type="sop_elements" mode="index"/>
        </documents>
        <nodes>
            <node hostalias="node1" distribution-key="0"/>
        </nodes>
        <engine>
            <proton>
                <searchable-copies>1</searchable-copies>
            </proton>
        </engine>
    </content>
</services>
